<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Base.Gean">#68BB86</Color>
    <Color x:Key="BorderMediumColor">#FF888888</Color>
    <Color x:Key="ControlLightColor"></Color>
    <Color x:Key="DisabledControlLightColor">#FFE8EDF9</Color>
    <Color x:Key="DisabledControlDarkColor">#FFC5CBF9</Color>
    <Color x:Key="DisabledForegroundColor">#FF888888</Color>
    <Color x:Key="DisabledBorderLightColor">#FFAAAAAA</Color>
    <Color x:Key="SelectedBackgroundColor">#3D9BE4B5</Color>
    <Color x:Key="MouseOverBackgroundColor">#1D9BE4B5</Color>

    <SolidColorBrush x:Key="Base.Background.Color" Color="#414141"/>
    <SolidColorBrush x:Key="Base.Panel.Color" Color="#363636"/>
    <SolidColorBrush x:Key="Base.Foreground.Color" Color="#CBCBCB"/>
    <SolidColorBrush x:Key="Icon.Color" Color="{StaticResource Base.Gean}"/>
    <SolidColorBrush x:Key="Button.Add.Color" Color="{StaticResource Base.Gean}"/>
    <SolidColorBrush x:Key="SelectedBackground" Color="{StaticResource SelectedBackgroundColor}"/>
    <SolidColorBrush x:Key="MouseOverBackground" Color="{StaticResource MouseOverBackgroundColor}"/>

</ResourceDictionary>